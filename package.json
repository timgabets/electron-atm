{
  "name": "electron-atm",
  "productName": "Electron ATM",
  "version": "1.4.3",
  "description": "A simple APTRA Advance NDC ATM emulator",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "test": "eslint tests/** src/services/** src/controllers/** && nyc --reporter=html --reporter=lcov ava",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "win32": "electron-builder --win",
    "linux": "electron-builder --linux"
  },
  "repository": "https://github.com/timgabets/electron-atm",
  "keywords": [
    "Electron",
    "ATM"
  ],
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "author": {
    "name": "Tim Gabets",
    "email": "tim@gabets.ru",
    "url": "http://gabets.ru/"
  },
  "license": "LGPL-2.1",
  "devDependencies": {
    "ava": "^0.24.0",
    "electron": "~10.1.3",
    "electron-builder": "^19.47.1",
    "eslint": "^4.12.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdom": "^11.5.1",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2"
  },
  "dependencies": {
    "atm-cursor": "^0.1.1",
    "atm-fits": "^0.2.0",
    "atm-hardware": "^0.2.3",
    "atm-logging": "^0.2.0",
    "atm-opcode-buffer": "^1.0.1",
    "atm-screens": "^0.1.1",
    "atm-screentext": "^0.1.0",
    "atm-state-levels": "^1.0.6",
    "atm-states": "^0.7.0",
    "atm-timestamp": "^1.1.0",
    "atm-trace": "^0.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^3.3.7",
    "electron-settings": "^3.1.1",
    "jquery": "^3.2.1",
    "jquery-validation": "^1.17.0",
    "mousetrap": "^1.6.1",
    "ndc-parser": "^0.2.0",
    "node-cardcrypto": "^1.0.2",
    "pinblock": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "vis": "^4.20.1"
  },
  "build": {
    "appId": "com.electron-atm"
  }
}
